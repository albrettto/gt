{
  "name": "@grafit/components",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc -p tsconfig.schematics.json",
    "copy:schemas": "xcopy schematics\\my-service\\schema.json ..\\..\\dist\\itsk-ng-lib\\schematics\\my-service",
    "copy:files": "xcopy /e /v schematics\\my-service\\files ..\\..\\dist\\itsk-ng-lib\\schematics\\my-service\\files\\",
    "copy:collection": "copy schematics\\collection.json ..\\..\\dist\\itsk-ng-lib\\schematics\\collection.json",
    "template:schemas": "xcopy schematics\\template\\schema.json ..\\..\\dist\\itsk-ng-lib\\schematics\\template",
    "template:files": "xcopy /e /v schematics\\template\\files ..\\..\\dist\\itsk-ng-lib\\schematics\\template\\files\\",
    "postbuild": "npm run copy:schemas && npm run copy:files && npm run copy:collection && npm run template:schemas && npm run template:files"
  },
  "peerDependencies": {
    "@angular/cdk": "^15.0.0",
    "@angular/common": "^16.2.0",
    "@angular/core": "^16.2.0",    
    "moment-mini": "^2.24.0"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "sideEffects": false
}
