{
  "$schema": "../../../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "my-service": {
      "description": "Generate a service in the project.",
      "factory": "./my-service/index#myService",
      "schema": "./my-service/schema.json"
    },
    "template": {
      "description": "Generate source code for template application",
      "factory": "./template/index#template",
      "schema": "./template/schema.json"
    },
    "itskPackages": {
      "description": "Add packages to project",
      "factory": "./itsk-packages/index#itskPackages"
    }
  }
}
